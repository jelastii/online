      "cell_type": "code",
      "source": [
        "rfm = data.groupby('CustomerID').agg({\n",
        "    'InvoiceDay': lambda x: (pin_date - x.max()).days,\n",
        "    'InvoiceNo': 'count',\n",
        "    'TotalSum': 'sum'\n",
        "})\n",
